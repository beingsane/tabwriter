{
  "name": "tabwriter",
  "private": true,
  "description": "Uma aplicação web para escrever tablaturas",
  "scripts": {
    "postinstall": "npm run bootstrap",
    "bootstrap": "lerna bootstrap --hoist",
    "build": "lerna run build --stream",
    "build:clean": "lerna run build:clean --stream",
    "test": "lerna run test --stream",
    "test:coverage": "lerna run test:coverage --stream",
    "test:clean": "lerna run test:clean --stream",
    "lint": "lerna run lint --stream",
    "lint:fix": "lerna run lint:fix --stream",
    "dev": "lerna run dev --parallel",
    "dev:server": "lerna run --scope=@tabwriter/server dev --stream",
    "dev:client": "lerna run --scope=@tabwriter/client dev --stream"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/raphael-jorge/tabwriter.git"
  },
  "bugs": {
    "url": "https://github.com/raphael-jorge/tabwriter/issues"
  },
  "homepage": "https://github.com/raphael-jorge/tabwriter#readme",
  "devDependencies": {
    "@types/jest": "^24.9.1",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "dotenv": "^8.2.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.5",
    "jest": "^24.9.0",
    "lerna": "^3.22.1",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.4",
    "ts-node": "^8.10.2"
  },
  "dependencies": {
    "@tabwriter/client": "file:packages/client",
    "@tabwriter/server": "file:packages/server"
  }
}
