{
  "name": "tabwriter-server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "clean": "rimraf build",
    "dev": "nodemon --watch \"src/\" --exec \"ts-node -r dotenv/config src/app.ts\" -e ts",
    "start": "node build/app.js",
    "test": "jest --no-cache",
    "test:coverage": "jest --no-cache --coverage",
    "test:clean": "rimraf coverage"
  },
  "dependencies": {
    "@types/express": "^4.17.6",
    "@types/morgan": "^1.9.0",
    "@types/node": "^13.13.5",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "http-status-codes": "^1.4.0",
    "morgan": "^1.10.0",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^25.5.4",
    "nodemon": "^2.0.3",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.5.1",
    "ts-node": "^8.10.1"
  }
}
