{
  "name": "tabwriter-server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "clean": "rimraf build",
    "dev": "nodemon --watch \"src/\" --exec \"ts-node -r dotenv/config src/app.ts\" -e ts",
    "start": "node build/app.js",
    "test": "jest --no-cache",
    "test:coverage": "jest --no-cache --coverage",
    "test:clean": "rimraf coverage"
  },
  "dependencies": {
    "@types/express": "^4.17.2",
    "@types/morgan": "^1.7.37",
    "@types/node": "^13.5.0",
    "express": "^4.17.1",
    "http-status-codes": "^1.4.0",
    "morgan": "^1.9.1",
    "typescript": "^3.7.5"
  },
  "devDependencies": {
    "@types/jest": "^25.1.0",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.1",
    "supertest": "^4.0.2",
    "ts-jest": "^25.0.0",
    "ts-node": "^8.6.2"
  }
}
