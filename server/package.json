{
  "name": "tabwriter-server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "clean": "rimraf build",
    "dev": "nodemon --watch \"src/\" --exec \"ts-node -r dotenv/config src/app.ts\" -e ts",
    "start": "node build/app.js",
    "test": "jest --no-cache",
    "test:coverage": "jest --no-cache --coverage",
    "test:clean": "rimraf coverage",
    "lint": "eslint src/**",
    "lint:fix": "eslint --fix src/**"
  },
  "dependencies": {
    "@types/express": "^4.17.6",
    "@types/morgan": "^1.9.0",
    "@types/node": "^14.0.6",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "express-validator": "^6.5.0",
    "http-status-codes": "^1.4.0",
    "morgan": "^1.10.0",
    "typescript": "^3.9.3"
  },
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "@types/supertest": "^2.0.9",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "dotenv": "^8.2.0",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^26.0.1",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.0",
    "ts-node": "^8.10.2"
  }
}
