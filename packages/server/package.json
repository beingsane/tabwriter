{
  "name": "@tabwriter/server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "build:clean": "rimraf build",
    "test": "jest --no-cache",
    "test:coverage": "jest --no-cache --coverage",
    "test:clean": "rimraf coverage",
    "lint": "eslint \"src/**/*.{js,ts}\"",
    "lint:fix": "eslint --fix \"src/**/*.{js,ts}\"",
    "dev": "nodemon --watch \"src/\" --exec \"ts-node -r dotenv/config src/app.ts\" -e ts",
    "prestart": "npm run build",
    "start": "node build/app.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/raphael-jorge/tabwriter.git",
    "directory": "packages/server"
  },
  "dependencies": {
    "@types/express": "^4.17.7",
    "@types/helmet": "0.0.47",
    "@types/morgan": "^1.9.1",
    "@types/node": "^12.12.53",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "express-validator": "^6.6.0",
    "helmet": "^4.0.0",
    "http-status-codes": "^1.4.0",
    "morgan": "^1.10.0",
    "typescript": "^3.9.7"
  }
}
